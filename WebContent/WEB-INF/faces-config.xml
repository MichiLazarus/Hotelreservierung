<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>hotelmanagement</managed-bean-name>
		<managed-bean-class>hotel2013.hm.Hotelmanagement</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bookingDAO</managed-bean-name>
		<managed-bean-class>hotel2013.hm.dao.BookingDAO</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>personDAO</managed-bean-name>
		<managed-bean-class>hotel2013.hm.dao.PersonDAO</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>roomDAO</managed-bean-name>
		<managed-bean-class>hotel2013.hm.dao.RoomDAO</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>booking</managed-bean-name>
		<managed-bean-class>hotel2013.hm.data.Booking</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>room</managed-bean-name>
		<managed-bean-class>hotel2013.hm.data.Room</managed-bean-class>
