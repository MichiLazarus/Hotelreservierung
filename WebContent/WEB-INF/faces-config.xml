	</managed-bean>
	<managed-bean>
		<managed-bean-name>analyst</managed-bean-name>
		<managed-bean-class>hotel2013.hm.users.Analyst</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customer</managed-bean-name>
		<managed-bean-class>hotel2013.hm.users.Customer</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>hotelier</managed-bean-name>
		<managed-bean-class>hotel2013.hm.users.Hotelier</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>person</managed-bean-name>
		<managed-bean-class>hotel2013.hm.users.Person</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
